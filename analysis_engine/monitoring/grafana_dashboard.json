{
  "dashboard": {
    "id": null,
    "uid": "ai-threat-hunting",
    "title": "AI Threat Hunting Simulator - Performance Dashboard",
    "tags": ["security", "threat-hunting", "ai", "performance"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "type": "stat",
        "title": "Total Analysis Requests",
        "targets": [
          {
            "expr": "sum(analysis_requests_total)",
            "refId": "A",
            "legendFormat": "Total Requests"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "gridPos": {
          "x": 6,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "type": "stat",
        "title": "Success Rate",
        "targets": [
          {
            "expr": "sum(rate(analysis_requests_total{status=\"success\"}[5m])) / sum(rate(analysis_requests_total[5m])) * 100",
            "refId": "A",
            "legendFormat": "Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 3,
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "type": "stat",
        "title": "Current Analysis Jobs",
        "targets": [
          {
            "expr": "current_analysis_jobs",
            "refId": "A",
            "legendFormat": "Active Jobs"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 10
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "type": "stat",
        "title": "Total Events Processed",
        "targets": [
          {
            "expr": "sum(events_processed_total)",
            "refId": "A",
            "legendFormat": "Events"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "blue",
                  "value": null
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "gridPos": {
          "x": 0,
          "y": 4,
          "w": 12,
          "h": 8
