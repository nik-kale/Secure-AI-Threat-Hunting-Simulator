        "type": "graph",
        "title": "Analysis Request Rate",
        "targets": [
          {
            "expr": "rate(analysis_requests_total{status=\"success\"}[5m])",
            "refId": "A",
            "legendFormat": "Success - {{scenario}}"
          },
          {
            "expr": "rate(analysis_requests_total{status=\"failure\"}[5m])",
            "refId": "B",
            "legendFormat": "Failure - {{scenario}}"
          }
        ],
        "yaxes": [
          {
            "label": "Requests/sec",
            "format": "short"
          },
          {
            "format": "short"
          }
        ]
      },
